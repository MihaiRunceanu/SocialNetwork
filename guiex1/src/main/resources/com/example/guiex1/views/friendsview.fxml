<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="576.0" prefWidth="828.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.guiex1.controller.PrietenieController">
    <children>
        <TableView fx:id="tableView" layoutX="30.0" layoutY="27.0" prefHeight="374.0" prefWidth="717.0">
            <columns>
                <TableColumn fx:id="tableColumnFirstName" prefWidth="375.0" text="FirstName" />
                <TableColumn fx:id="tableColumnLastName" prefWidth="393.0" text="LastName" />
            </columns>
        </TableView>
        <ButtonBar layoutX="10.0" layoutY="430.0" prefHeight="40.0" prefWidth="802.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#handleRequests" prefHeight="40.0" prefWidth="136.0" text="Friend Requests">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
                <Button minWidth="0.0" mnemonicParsing="false" onAction="#handleRemove" prefHeight="60.0" prefWidth="218.0" text="Remove Friend">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onAction="#handleAdd" prefHeight="56.0" text="Add Friend">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleMessage" prefHeight="56.0" text="Message">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleOpenMyProfile" prefHeight="40.0" text="MyProfile">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            </buttons>
        </ButtonBar>
      <Button fx:id="buttonPrevious" layoutX="30.0" layoutY="510.0" mnemonicParsing="false" onAction="#onPreviousPage" prefHeight="26.0" prefWidth="61.0" text="Previous" />
      <Button fx:id="buttonNext" layoutX="149.0" layoutY="510.0" mnemonicParsing="false" onAction="#onNextPage" prefHeight="26.0" prefWidth="52.0" text="Next" />
      <Label fx:id="labelPage" layoutX="92.0" layoutY="542.0" text="Label" />
      <ButtonBar layoutX="486.0" layoutY="503.0" prefHeight="40.0" prefWidth="321.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleUploadPfp" prefHeight="40.0" prefWidth="153.0" text="Upload Photo">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleOpenProfile" prefHeight="50.0" text="Friend Profile">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
    </children>
</AnchorPane>
